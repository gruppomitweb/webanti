TEMPLATE portalstudio.css

.\controlName\ {
  background-color:\portalZoomConfigurator_backgroundColor!\;
  background-image:\portalZoomConfigurator_backgroundImage!\;
  background-repeat:\portalZoomConfigurator_backgroundRepeat!\;
  background-position:\portalZoomConfigurator_backgroundPosition!\;
}

.\controlName\ .toolbar{
  background-color:\portalZoomConfigurator_toolbar_backgroundColor!\;
  background-image:\portalZoomConfigurator_toolbar_backgroundImage!\;
  background-repeat:\portalZoomConfigurator_toolbar_backgroundRepeat!\;
  background-position:\portalZoomConfigurator_toolbar_backgroundPosition!\;
  height:\portalZoomConfigurator_toolbar_height!\;
  display: flex;
  flex-flow:row nowrap;
  align-items:center;
}

.\controlName\ .searchBox{
  display:inline-table;
}

.\controlName\ .searchBox{
  width:100%;
  box-sizing:border-box;
  font-family:\portalZoomConfigurator_search_font!\;
  font-size:\portalZoomConfigurator_search_fontSize!\;
  font-weight:\portalZoomConfigurator_search_fontWeight!\;
  text-align:\portalZoomConfigurator_search_textAlign!\;
  color:\portalZoomConfigurator_search_color!\;
  background-color:\portalZoomConfigurator_search_backgroundColor!\;
  border-width:\portalZoomConfigurator_search_borderSize!\;
  border-color:\portalZoomConfigurator_search_borderColor!\;
  border-style:\portalZoomConfigurator_search_borderStyle!\;
  border-radius:\portalZoomConfigurator_search_borderRadius!\;
  padding:\portalZoomConfigurator_search_padding!\;
  margin:\portalZoomConfigurator_search_margin!\;
}

.\controlName\ .toolbar span{
  font-family:\portalZoomConfigurator_toolbar_accept_font!\;
  font-size:\portalZoomConfigurator_toolbar_accept_fontSize!\;
  font-weight:\portalZoomConfigurator_toolbar_accept_fontWeight!\;
  color:\portalZoomConfigurator_toolbar_accept_color!\;
  padding:\portalZoomConfigurator_toolbar_accept_padding!\;
  margin:\portalZoomConfigurator_toolbar_accept_margin!\;
}

.\controlName\ .toolbar a{  
  padding:4px;
}

.\controlName\ .toolbar .searchDiv{
  flex:1;
}

.\controlName\ .columnListContainer{
  border-width:\portalZoomConfigurator_columnListContainer_borderSize!\;
  border-color:\portalZoomConfigurator_columnListContainer_borderColor!\;
  border-style:\portalZoomConfigurator_columnListContainer_borderStyle!\;
  border-radius:\portalZoomConfigurator_columnListContainer_borderRadius!\;
  margin:\portalZoomConfigurator_columnListContainer_margin!\;
  padding:\portalZoomConfigurator_columnListContainer_padding!\;
}
.\controlName\ .columnList:first-child{
  border-top:0;
}
.\controlName\ .columnList:last-child{
  border-bottom:0;
}

.\controlName\ .columnList{
  display: block;
  cursor:pointer;
  padding-left:10px;
  font-family:\portalZoomConfigurator_columnList_font!\;
  font-size:\portalZoomConfigurator_columnList_fontSize!\;
  font-weight:\portalZoomConfigurator_columnList_fontWeight!\;
  text-align:\portalZoomConfigurator_columnList_textAlign!\;
  color:\portalZoomConfigurator_columnList_color!\;
  background-color:\portalZoomConfigurator_columnList_backgroundColor!\;
  border-width:\portalZoomConfigurator_columnList_borderSize!\;
  border-color:\portalZoomConfigurator_columnList_borderColor!\;
  border-style:\portalZoomConfigurator_columnList_borderStyle!\;
  padding:\portalZoomConfigurator_columnList_padding!\;
  line-height:\portalZoomConfigurator_columnList_lineHeight!\;
}

.\controlName\ .columnList.selected{
  color:\portalZoomConfigurator_columnList_selected_color!\;
  background-color:\portalZoomConfigurator_columnList_selected_backgroundColor!\;
}

.\controlName\ .columnList .checkColumn{
  margin:\portalZoomConfig_columnList_SelectedColumn_margin!\;
}

.\controlName\ .tab_container{
  font-family:\portalZoomConfigurator_tab_font!\;
  font-size:\portalZoomConfigurator_tab_fontSize!\;
  font-weight:\portalZoomConfigurator_tab_fontWeight!\;
  text-align:\portalZoomConfigurator_tab_textAlign!\;
  color:\portalZoomConfigurator_tab_color!\;
  background-color:\portalZoomConfigurator_tab_backgroundColor!\;

.SPPortalZoomBaseOL_portlet.stretch_height.SPPortalZoom, .SPPortalZoomBase_portlet.stretch_height.SPPortalZoom {
  height:100%;
}
.SPPortalZoomBaseOL_container.stretch_height, .SPPortalZoomBase_container.stretch_height {
  height:100%;
}
.SPPortalZoom_currentversion {
color: \portalZoomCurrentVersionLabelColor!\;
font-family: \portalZoomCurrentVersionLabelFontFamily!\;
font-weight: \portalZoomCurrentVersionLabelFontWeight!\;
font-size: \portalZoomCurrentVersionLabelFontSize!\;
padding: \portalZoomCurrentVersionLabelPadding!\;
}

.SPPortalZoom_versionhandler {
  color: \portalZoomVersionList.Color!\;
  font-family: \portalZoomVersionList.FontName!\;
  font-size: \portalZoomVersionList.Size!\px;
  padding: \portalZoomVersionListPadding!\;
  margin: \portalZoomVersionListMargin!\;
  vertical-align: \portalZoomVersionListVerticalAlign!\;
  cursor: \portalZoomVersionListCursor!\;
  display:inline-block;
}
